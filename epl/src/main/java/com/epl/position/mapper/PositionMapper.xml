<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "com.epl.position.mapper.PositionMapper">
	<select id="selectPositionNo" resultType="com.epl.vo.Position" parameterType="com.epl.vo.Position">
		SELECT position_no as positionNo
		FROM position WHERE position =#{position}
	</select>
</mapper>