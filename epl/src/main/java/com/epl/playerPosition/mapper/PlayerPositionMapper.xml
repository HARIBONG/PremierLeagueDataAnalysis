<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "com.epl.playerPosition.mapper.PlayerPositionMapper">
	<insert id="insertPlayerPosition" parameterType="com.epl.vo.PlayerPosition">
		INSERT INTO player_position(
			player_no,
			position_no
		) VALUES (
			#{playerNo} ,
			#{positionNo}
		)
	</insert>
</mapper>